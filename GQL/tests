mutation AddPlaylist {
  addPlaylist(ownerURI:"123", ownerName: "Sammy", playlistURI: "321", playlistName: "OG KUSH", image: "no901", 
    songs: [
      {id: "123", name: "abc", image: "hi", artist: "hiiiii", duration: 5}
    ]
  ) {
    id
    ownerName
    ownerURI
    playlistName
    songs {
      id
      name
      image
    }
    live
    currentSong
    image
  }
}

query AllPlaylists {
  getPlaylists {
    playlists {
      id
      ownerName
    }
  }
}

query GetSomePlaylists {
  getPlaylistsOf(ids: ["24c40b3e-3cae-42a7-9dff-ab0a0f882a70", "1"]) {
    id
    ownerName
    currentSong
    songs {
      id
    }
    live
  }
}

query GetPlaylist {
  getPlaylist(id: "24c40b3e-3cae-42a7-9dff-ab0a0f882a70") {
    id
    ownerName
    currentSong
    songs {
      id
    }
  }
}

